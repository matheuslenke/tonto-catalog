import Transaction
import Decentralized
import Recurring
import Price_Calculation

package Price_Discovery

mode Price 
subkind Suggested_Price  specializes Price{
    [1] -- [1]  Transaction
     [0..*] -- towards -- [1]  Offering_On_The_Platform
}
subkind Market_Price  specializes Price
subkind Offering_Price  specializes Price{
     [0..*] -- has -- [1]  Listing
    [1] -- has --  [0..*]  Time_Slot
    @subQuantityOf
    ( {const } ) [1..*]  -- used_in --<o>  [0..*]  Booking_Price
}

