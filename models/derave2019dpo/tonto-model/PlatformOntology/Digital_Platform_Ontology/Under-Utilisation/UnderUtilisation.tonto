import Transaction
import Productoriented

package UnderUtilisation

subkind Physical_Good  specializes Good
event Regularly_uses {
    @participation
     [0..*] -- performed_by -- [1]  Platform_Provider( {const } )
    @participation
     [1..*] -- includes --  [1..*]  UnderUtilized_Product( {const } )
}
subkind UnderUtilized_Product  specializes Physical_Good

