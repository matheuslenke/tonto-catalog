@prefix : <https://example.com#>.
@prefix gufo: <http://purl.org/nemo/gufo#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

<https://example.com> rdf:type owl:Ontology;
    owl:imports gufo:.
:Father rdf:type owl:Class, gufo:Role, owl:NamedIndividual;
    rdfs:label "Father"@en.
:FemalePerson rdf:type owl:Class, gufo:SubKind, owl:NamedIndividual;
    rdfs:label "FemalePerson"@en.
:MalePerson rdf:type owl:Class, gufo:SubKind, owl:NamedIndividual;
    rdfs:label "MalePerson"@en.
:Mother rdf:type owl:Class, gufo:Role, owl:NamedIndividual;
    rdfs:label "Mother"@en.
:Offspring rdf:type owl:Class, gufo:Role, owl:NamedIndividual;
    rdfs:label "Offspring"@en.
:Parent rdf:type owl:Class, gufo:Role, owl:NamedIndividual;
    rdfs:label "Parent"@en.
:Person rdf:type owl:Class, gufo:Kind, owl:NamedIndividual;
    rdfs:subClassOf gufo:FunctionalComplex;
    rdfs:label "Person"@en.
:Registration rdf:type owl:Class, gufo:Kind, owl:NamedIndividual;
    rdfs:subClassOf gufo:Relator;
    rdfs:label "Registration"@en.
:Father rdfs:subClassOf :Parent, :MalePerson.
:FemalePerson rdfs:subClassOf :Person.
:MalePerson rdfs:subClassOf :Person.
:Mother rdfs:subClassOf :FemalePerson, :Parent.
:Offspring rdfs:subClassOf :Person.
:Parent rdfs:subClassOf :Person.
