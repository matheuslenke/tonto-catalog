    [0..*] -- tem -- [0..*] Diagnstico_COVID19 
    [0..*] -- sofre_de -- [0..*] Doena 
    [0..*] -- tem -- [*] Sintoma 
    [0..*] -- reside_em -- [0..*] Estabelecimento 
    [0..*] -- trabalha_em -- [0..*] Estabelecimento 
    [0..*] -- viaja_para -- [0..*] Lugar_Geogrfico 
}

mode Disfuno{
