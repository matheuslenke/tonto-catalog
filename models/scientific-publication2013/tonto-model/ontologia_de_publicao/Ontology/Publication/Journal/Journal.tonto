import Publication

package Journal

kind Journal{
    periodicity: undefined  
    title: undefined  
    subtitle: undefined  
    @material [1] -- [0..*] Journal_Issue 
}

kind Journal_Issue specializes Periodical {
    volume: undefined  
    number_: undefined  
    issueDate: undefined  
    issn: undefined  
    @componentOf [1] -- [2..*] Article 
}

kind Article specializes Publication, Authored_Document {
    startPage: undefined  
    endPage: undefined  
    abstract: undefined  
}

