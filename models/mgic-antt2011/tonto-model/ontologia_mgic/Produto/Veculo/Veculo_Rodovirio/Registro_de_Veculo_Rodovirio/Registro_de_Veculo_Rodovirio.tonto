import Tipo_de_Veculo
import Transportador_Rodovirio_de_Cargas

package Registro_de_Veculo_Rodovirio

relator Registro_de_Veculo {
    endereo : undefined
    nmero : undefined
    @mediation
    [1] -- [1]  Veculo_Rodovirio( {const } )
    @mediation
     [1..*] -- [1]  Proprietrio_de_Veculo( {const } )
}
roleMixin Proprietrio_de_Veculo  specializes Transportador_de_Carga_Prpria{
    @material
    [1] -- proprietrio_de --  [1..*]  Veculo_Rodovirio
}

