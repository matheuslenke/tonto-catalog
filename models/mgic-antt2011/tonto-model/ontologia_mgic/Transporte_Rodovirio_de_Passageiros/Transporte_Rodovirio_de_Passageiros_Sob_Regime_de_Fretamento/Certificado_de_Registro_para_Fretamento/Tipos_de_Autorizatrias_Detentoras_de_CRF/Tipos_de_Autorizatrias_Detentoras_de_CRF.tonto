import Tipo_de_Autorizao_de_Viagem
import Fretamento_Contnuo
import Fretamento_Eventual_ou_Turstico
import Certificado_de_Registro_para_Fretamento

package Tipos_de_Autorizatrias_Detentoras_de_CRF

role Autorizatria_com_Autorizao_para_Fretamento_Eventual_ou_Turstico  specializes Autorizatria_Detentora_de_CRF{
    @mediation
    ( {const } )[1] --  [1..*]  Autorizao_de_Viagem_para_Fretamento_Eventual_ou_Turstico
    @material
     [1..*] -- fornece_autorizao --  [1..*]  rgo_Conveniado_Emissor_de_Autorizao_para_Fretamento_Eventual_ou_Turstico
}
role Autorizatria_com_Autorizao_para_Fretamento_Contnuo  specializes Autorizatria_Detentora_de_CRF{
    @mediation
    ( {const } autorizataria )[1] --  [1..*]  Autorizao_de_Viagem_para_Fretamento_Contnuo
    @mediation
    ( {const } )[1] --  [1..*]  Fretamento_Contnuo
    @material
    [1] -- contrata_ --  [1..*]  Contratante_Fretamento_Contnuo
    @material
     [1..*] -- fornece_autorizao --  [1..*]  Agncia_Reguladora_Emissora_de_Autorizao_para_Fretamento_Contnuo
}

