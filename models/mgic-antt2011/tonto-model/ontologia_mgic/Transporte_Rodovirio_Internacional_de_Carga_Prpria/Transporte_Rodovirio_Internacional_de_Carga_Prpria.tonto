import Transportador_Rodovirio_de_Cargas
import Transporte_Rodovirio_Internacional_de_Cargas

package Transporte_Rodovirio_Internacional_de_Carga_Prpria

roleMixin Carga_em_Transporte_Rodovirio 
relator Operao_de_Transporte_Rodovirio_Internacional_de_Carga_Prpria  specializes Operao_de_Transporte_Rodovirio_Internacional_de_Cargas{
    @mediation
    [1] -- [1]  Veculo_em_Transporte_Rodovirio_Internacional_de_Carga_Prpria( {const } )
    @mediation
     [1..*] -- [1]  Transportador_de_Carga_Prpria_em_Territrio_Internacional( {const } )
}
role Veculo_em_Transporte_Rodovirio_Internacional_de_Carga_Prpria 
role Transportador_de_Carga_Prpria_em_Territrio_Internacional  specializes Transportador_de_Carga_Prpria
relator Propriedade_de_Carga {
    @mediation
     [1..*] -- [1]  Transportador_de_Carga_Prpria( {const } )
    @mediation
    [1] -- [1]  Carga_em_Transporte_Rodovirio( {const } )
}

