relator Acordo_Formal_de_Contratao_de_Servio_de_Transporte_Rodovirio_de_Cargas {
    @mediation
    [1] -- [1]  Contratante_de_Prestao_de_Servio_de_Transporte_Rodovirio_Nacional_de_Cargas( {const } )
    @mediation
    [1] -- [1]  Contratado_de_Prestao_de_Servio_de_Transporte_Rodovirio_Nacional_de_Cargas( {const } )
}
role CTe_em_Operao_de_Transporte_Rodovirio_Nacional_de_Cargas  specializes Documento_Fiscal_Vlido_para_Operao_de_Transporte_Rodovirio_Nacional_de_Cargas
relator Operao_de_Transporte_Rodovirio_Nacional_de_Cargas_Mediante_Frete  specializes Operao_de_Transporte_Rodovirio_Nacional_de_Cargas{
    @mediation
    [1] -- [1]  Veculo_Exclusivamente_de_Carga_em_Transporte_de_Cargas( {const } veiculoExclusivamenteCarga )
    @mediation
    [1] -- [1]  DACTE_Emitido( {const } )
    @mediation
     [1..*] -- [1]  Contratado_de_Prestao_de_Servio_de_Transporte_Rodovirio_Nacional_de_Cargas( {const } contratado )
    @mediation
     [1..*] -- [1]  Contratante_de_Prestao_de_Servio_de_Transporte_Rodovirio_Nacional_de_Cargas( {const } )
    @Formal
    [1] -- referese_a --  [0..1]  Acordo_Formal_de_Contratao_de_Servio_de_Transporte_Rodovirio_de_Cargas
}
roleMixin Contratante_de_Prestao_de_Servio_de_Transporte_Rodovirio_Nacional_de_Cargas  specializes Contratante_de_Prestao_de_Servio_de_Transporte_Rodovirio_de_Cargas
