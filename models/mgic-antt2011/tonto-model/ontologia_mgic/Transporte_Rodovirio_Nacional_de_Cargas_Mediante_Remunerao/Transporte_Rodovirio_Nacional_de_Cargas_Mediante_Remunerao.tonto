role Pessoa_Jurdica_Contratante_de_Prestao_de_Servio_de_Transporte_Rodovirio_Nacional_de_Cargas  specializes Contratante_de_Prestao_de_Servio_de_Transporte_Rodovirio_Nacional_de_Cargas
relator Operao_de_Transporte_Rodovirio_Nacional_de_Cargas_Mediante_Frete_com_Documentao_Fiscal  specializes Operao_de_Transporte_Rodovirio_Nacional_de_Cargas_Mediante_Frete{
    @mediation
    [1] --  [1..*]  Documento_Fiscal_Vlido_para_Operao_de_Transporte_Rodovirio_Nacional_de_Cargas( {const } )
}
