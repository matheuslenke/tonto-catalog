     [1..*] -- [1]  Condio_de_Manuteno_de_Ativos( {const } )
}
mode Demanda_Firme {
    @characterization
     [0..1] -- [1]  Contrato_de_Operao_Especfica( {const } )
}
mode Definio_de_Caracterstica_da_Via_Permanente {
    @characterization
     [1..*] -- [1]  Contrato_de_Operao_Especfica( {const } )
}

