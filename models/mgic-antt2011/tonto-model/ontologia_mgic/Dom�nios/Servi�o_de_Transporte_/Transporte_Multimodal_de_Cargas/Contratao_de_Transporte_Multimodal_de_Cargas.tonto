import Tipos_de_Conhecimento_de_Transporte
import Operador_de_Transporte_Multimodal

package Contratao_de_Transporte_Multimodal_de_Cargas

roleMixin Contratante_de_Transporte_Multimodal_de_Cargas 
role Operador_de_Transporte_Multimodal_Contratado  specializes Operador_de_Transporte_Multimodal{
    @material
    [1] -- emite -- [1]  Conhecimento_de_Transporte_Multimodal
}
relator Contratao_de_Transporte_Multimodal_de_Cargas {
    @mediation
     [1..*] -- [1]  Operador_de_Transporte_Multimodal_Contratado( {const } )
    @mediation
     [1..*] -- [1]  Contratante_de_Transporte_Multimodal_de_Cargas( {const } )
}
role Pessoa_Jurdica_Contratante_de_Transporte_Multimodal_de_Cargas  specializes Contratante_de_Transporte_Multimodal_de_Cargas
role Pessoa_Fsica_Contratante_de_Transporte_Multimodal_de_Cargas  specializes Contratante_de_Transporte_Multimodal_de_Cargas

