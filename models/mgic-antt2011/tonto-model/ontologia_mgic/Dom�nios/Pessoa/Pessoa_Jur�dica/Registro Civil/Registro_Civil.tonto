import Pessoa

package Registro_Civil

role Representante_Legal 
role Cartrio_Registrador_de_Pessoa_Jurdica 
relator Registro_Civil_de_Pessoa_Jurdica {
    @mediation
    [1] -- [1]  Pessoa_Jurdica( {const } )
    @mediation
     [1..*] -- [1]  Representante_Legal( {const } )
    @mediation
     [1..*] -- [1]  Cartrio_Registrador_de_Pessoa_Jurdica( {const } )
}

