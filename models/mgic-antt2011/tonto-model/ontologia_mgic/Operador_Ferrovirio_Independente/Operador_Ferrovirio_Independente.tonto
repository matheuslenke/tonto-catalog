import Concesso_Ferroviria
import Movimentao_de_Ativo_Ferrovirio

package Operador_Ferrovirio_Independente

role Concesso_Ferroviria_com_Operador_Ferrovirio_Independente  specializes Concessionria_Ferroviria
subkind Contrato_de_Subcesso_Onerosa_de_Direito_de_Uso_ 
role Operador_Ferrovirio_Independente {
    @material
     [1..*] -- autoriza_transporte -- [1]  Concesso_Ferroviria_com_Operador_Ferrovirio_Independente
}
relator Estabelecimento_de_Direito_de_Uso_para_Operador_Ferrovirio_Independente {
    @mediation
    [1] -- [1]  Concesso_Ferroviria_com_Operador_Ferrovirio_Independente( {const } )
    @mediation
     [1..*] -- [1]  Operador_Ferrovirio_Independente( {const } )
    @mediation
    [1] -- [1]  Contrato_de_Subcesso_Onerosa_de_Direito_de_Uso_( {const } )
}
role OFI_Operador_de_Viagem_Ferroviria  specializes Operador_Ferrovirio_Independente
relator Transporte_Ferrovirio_de_Carga_Operado_por_OFI  specializes Transporte_Ferrovirio_de_Carga{
    @mediation
     [1..*] -- [1]  OFI_Operador_de_Viagem_Ferroviria( {const } )
}

