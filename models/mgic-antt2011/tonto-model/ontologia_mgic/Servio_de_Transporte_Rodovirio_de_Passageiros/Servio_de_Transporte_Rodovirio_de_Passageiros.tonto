import Servio_de_Transporte_Regular_Rodovirio_de_Passageiros
import Via_de_Trnsito_Rodovirio

package Servio_de_Transporte_Rodovirio_de_Passageiros

role Trecho_Percorrido_por_Transporte_Rodovirio_de_Passageiros  specializes Trecho_Rodovirio_Fsico
subkind nibus_Executivo 
role Micronibus_em_Transporte_Rodovirio_de_Passageiros  specializes Veculo_em_Transporte_Rodovirio_de_Passageiros
relator TRP_Regular  specializes Transporte_Rodovirio_de_Passageiros{
    @mediation
    [1] -- [1]  Terminal_Rodovirio_Destino( {const } )
    @mediation
    [1] -- [1]  Terminal_Rodovirio_Origem( {const } )
}
subkind nibus_Leito 
role nibus_em_Transporte_Rodovirio_de_Passageiros  specializes Veculo_em_Transporte_Rodovirio_de_Passageiros
role Motorista 
subkind nibus_Semiurbano 
subkind nibus_Urbano 
subkind nibus_Convencional 
relator Transporte_Rodovirio_de_Passageiros {
    @mediation
     [1..*] --  [1..*]  Trecho_Percorrido_por_Transporte_Rodovirio_de_Passageiros( {const } )
}
roleMixin Veculo_em_Transporte_Rodovirio_de_Passageiros {
    @mediation
    ( {const } ) [1..*] -- [1]  Transporte_Rodovirio_de_Passageiros
    @material
    [1] -- transporta --  [1..*]  Passageiro
}
role Passageiro {
    @mediation
     [1..*] -- [1]  Transporte_Rodovirio_de_Passageiros( {const } passageiro_de_trp )
}
subkind nibus_Misto 
role Motorista_em_Transporte_de_Passageiros  specializes Motorista{
    @mediation
     [1..*] -- [1]  Transporte_Rodovirio_de_Passageiros( {const } motorista_de_trp )
}
subkind nibus_Semileito 

