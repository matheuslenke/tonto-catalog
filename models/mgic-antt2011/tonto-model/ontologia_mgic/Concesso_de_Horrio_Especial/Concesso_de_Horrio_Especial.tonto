import Agente_Administrativo
import Estgio

package Concesso_de_Horrio_Especial

role Servidor_em_Horrio_Especial_de_Estudante  specializes Servidor_Pblico_Efetivo, Estudante
relator Concesso_de_Horrio_Especial_para_Estudante  specializes Concesso_com_Compensao, Concesso_de_Horrio_Especial{
    @mediation
    [1] -- [1]  Servidor_em_Horrio_Especial_de_Estudante( {const } )
}
role _Servidor_em_Horrio_Especial_de_Portador_de_Deficincia  specializes Servidor_Pblico_Efetivo, Portador_de_Deficincia
collective Junta_Mdica 
role Servidor_em_Concesso_de_Horrio_Especial_por_Encargo  specializes Servidor_Pblico_Efetivo
relator Concesso_de_Horrio_Especial_para_Portador_de_Deficincia  specializes Concesso_de_Horrio_Especial{
    @mediation
    [1] -- [1]  _Servidor_em_Horrio_Especial_de_Portador_de_Deficincia( {const } )
}
relator Comprovao_de_Deficincia {
    @mediation
     [1..*] -- [1]  Responsvel_por_Comprovao_de_Deficincia( {const } )
    @mediation
    [1] -- [1]  Portador_de_Deficincia( {const } )
}
relator Concesso_de_Horrio_Especial {
    carga_horria : undefined
    compensao : undefined
    entrada : undefined
    prazo : undefined
    sada : undefined
}
relator Concesso_com_Compensao  specializes Concesso_de_Horrio_Especial{
    prazo_compensao : undefined
}
role Portador_de_Deficincia 
role Responsvel_por_Comprovao_de_Deficincia  specializes Junta_Mdica
relator Concesso_de_Horrio_Especial_por_Encargo_de_Curso_ou_Concurso  specializes Concesso_com_Compensao, Concesso_de_Horrio_Especial{
    @mediation
    [1] -- [1]  Servidor_em_Concesso_de_Horrio_Especial_por_Encargo( {const } )
}

