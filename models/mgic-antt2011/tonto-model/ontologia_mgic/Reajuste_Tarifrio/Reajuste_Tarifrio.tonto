import Produto
import Concesso_Ferroviria

package Reajuste_Tarifrio

kind Tabela_Tarifria {
    incio_de_vigncia : undefined
    @componentOf
    [1] <o>--  [2..*]  Produto_Componente_da_Tabela_Tarifria
}
role Tabela_Tarifria_Criada_a_Partir_de_Reviso  specializes Tabela_Tarifria
mode Reajuste_Tarifrio {
    IGPDI : undefined
    @characterization
    [1] -- [1]  Tabela_Tarifria_Criada_a_Partir_de_Reajuste( {const } )
}
kind Produto_Componente_da_Tabela_Tarifria {
    @componentOf
    [1] <o>-- [1]  Parcela_Varivel
    @componentOf
     [1..*] <o>-- [1]  Parcela_Fixa
    @Formal
     [0..1] -- [1]  Produto
}
role Clusulas_de_Contrato_de_Concesso_Cumpridas  specializes Clusula_de_Contrato_de_Concesso_Ferroviria{
    @Formal
     [1..*] -- cumpre -- [1]  Concessionria_Adimplente
}
kind Parcela_Fixa {
    unidade : undefined
    valor : undefined
}
role Concessionria_que_Pleiteia_Reajuste 
role Tabela_Tarifria_Criada_a_Partir_de_Reajuste  specializes Tabela_Tarifria
relator Pleito_de_Reajuste_Tarifrio {
    @mediation
     [1..*] -- [1]  Agncia_Reguladora_Avaliadora_de_Pleito_de_Reajuste( {const } )
    @mediation
    [1] -- [1]  Concessionria_que_Pleiteia_Reajuste( {const } pleito )
}
role Concessionria_Adimplente  specializes Concessionria_Ferroviria
role Agncia_Reguladora_Avaliadora_de_Pleito_de_Reajuste 
subkind Clusula_de_Contrato_de_Concesso_Ferroviria 
kind Parcela_Varivel {
    faixa_de_quilmetro_ : undefined
    unidade : undefined
    valor : undefined
}
relator Pleito_de_Reajuste_Tarifrio_Aceito  specializes Pleito_de_Reajuste_Tarifrio{
    @mediation
    [1] -- [1]  Tabela_Tarifria_Criada_a_Partir_de_Reajuste( {const } )
}

