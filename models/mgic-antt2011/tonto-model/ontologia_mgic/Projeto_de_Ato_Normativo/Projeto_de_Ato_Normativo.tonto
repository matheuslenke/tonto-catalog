import Atos_e_Artigos
import Sesso_do_Plenrio
import Pessoa
import Pessoa_Jurdica

package Projeto_de_Ato_Normativo

relator Anlise_Tcnica {
    @mediation
     [1..*] -- [1]  Projeto_de_Lei_com_Anlise_Tcnica( {const } )
    @mediation
     [1..*] -- [1]  Superintendncia_Responsvel_por_Anlise( {const } )
}
role Ato_Normativo_Aprovado  specializes Ato_Normativo
role Projeto_de_Lei_com_Anlise_Tcnica 
role Projeto_de_Lei_com_Anlise_Jurdica 
role Projeto_de_Ato_Normativo  specializes Ato_Normativo, Proposio, Projeto_de_Lei_com_Anlise_Tcnica, Projeto_de_Lei_com_Anlise_Jurdica{
    ano : undefined
}
mixin Agente_Legislativo  specializes Pessoa_Fsica, Partido_Poltico
roleMixin Autor_de_Projeto_de_Ato_Normativo  specializes Agente_Legislativo{
    @Formal
    [1] --  [1..*]  Projeto_de_Ato_Normativo
}
role Superintendncia_Responsvel_por_Anlise 

