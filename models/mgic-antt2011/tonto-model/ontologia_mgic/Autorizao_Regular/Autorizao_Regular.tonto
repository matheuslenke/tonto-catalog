import Autorizao
import Pessoa_Jurdica
import Outorga

package Autorizao_Regular

relator Outorga_de_Autorizao  specializes Outorga_sem_Licitao{
    @mediation
    [1] -- [1]  Contrato_de_Autorizao( {const } )
    @mediation
     [1..*] -- [1]  Autorizatria( {const } )
}
role Autorizatria  specializes Sociedade_de_Propsito_Especfico, Outorgado_Contigencialmente{
    @mediation
    ( {const } )[1] -- [1]  Outorga_sem_Licitao
}

