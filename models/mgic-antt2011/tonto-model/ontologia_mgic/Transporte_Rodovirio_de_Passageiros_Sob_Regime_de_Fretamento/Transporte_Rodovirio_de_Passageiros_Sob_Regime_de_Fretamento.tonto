import Tipos_de_Fretamento
import Servio_de_Transporte_Rodovirio_de_Passageiros

package Transporte_Rodovirio_de_Passageiros_Sob_Regime_de_Fretamento

role Motorista_de_TRP_Sob_Regime_de_Fretamento  specializes Motorista
relator Inspeo_Tcnica {
    dataEmisso : undefined
    dataInspeo : undefined
    dataVencimento : undefined
    @mediation
    [1] -- [1]  Laudo_de_Inspeo_Tcnica( {const } )
    @mediation
    [1] -- [1]  nibus_em_TRP_sob_regime_de_Fretamento( {const } )
}
role nibus_em_TRP_sob_regime_de_Fretamento  specializes Veculo_em_Transporte_Rodovirio_de_Passageiros{
    @mediation
     [1..*] -- [1]  TRP_Sob_Regime_de_Fretamento( {const } )
}
role Passageiro_de_TRP_Sob_Regime_de_Fretamento  specializes Passageiro{
    @mediation
     [1..*] -- [1]  TRP_Sob_Regime_de_Fretamento( {const } )
}
kind Laudo_de_Inspeo_Tcnica 
role Agente_Emissor_de_LIT {
    @material
    [1] -- emite_ --  [1..*]  Laudo_de_Inspeo_Tcnica
    @mediation
    [1] --  [1..*]  Inspeo_Tcnica( {const } )
}

