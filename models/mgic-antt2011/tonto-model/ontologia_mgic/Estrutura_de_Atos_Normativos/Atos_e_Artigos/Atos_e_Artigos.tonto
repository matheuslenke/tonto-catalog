package Atos_e_Artigos

subkind Artigo_Ordinrio  specializes Artigo{
    @componentOf
     [1..*]  -- essential_inseparable --<o> [1]  Ato_Normativo
}
subkind Decreto_Legislativo  specializes Ato_Normativo
subkind Artigo_Legal  specializes Artigo, Artigo_Legal_com_Infrao_Definida
kind Ementa {
    @componentOf
    [1]  -- essential_inseparable --<o> [1]  Ato_Normativo
}
