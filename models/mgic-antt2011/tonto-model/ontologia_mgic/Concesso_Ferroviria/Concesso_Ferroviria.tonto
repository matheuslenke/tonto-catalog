import Plano_de_Investimentos
import Faixa_de_Domnio_Ferroviria
import Trecho_Ferrovirio
import Bem_da_Concesso
import Sistema_de_Sinalizao_Ferroviria
import Meta_de_Concesso_Ferroviria
import Reajuste_Tarifrio
import Movimentao_de_Ativo_Ferrovirio
import Outorga
import Pessoa_Jurdica
import Contratos_e_Convnios

package Concesso_Ferroviria

subkind Concessionria_Ferroviria  specializes Concessionria_Ferroviria_que_Realiza_Aquisio, Concessionria_que_Pleiteia_Reajuste, Concessionria_Ferroviria_que_Realiza_Viagem_sem_Interseo{
    @material
    [1] -- tem_posse_ -- [1]  Malha_Ferroviria_Concedida
    @Formal
    [1] --  [1..*]  Sistema_de_Sinalizao_Ferroviria
    @characterization
    [1] -- [1]  Meta_de_Reduo_de_Acidente( {const } )
}
relator Outorga_de_Concesso_Ferroviria_Ativa  specializes Outorga_de_Concesso_Ferroviria{
    desempenho_operacional : undefined
    @mediation
    [1] -- [1]  Malha_Ferroviria_Concedida( {const } )
    @mediation
    [1] -- [1]  Concessionria_Ferroviria( {const } concessionaria )
}
relator Outorga_de_Concesso_Ferroviria_com_Contrato_de_Concesso_e_Contrato_de_Arrendamento_de_Bens  specializes Outorga_de_Concesso_Ferroviria_Ativa{
    @mediation
    [1] --  [1..*]  Ativo_Ferrovirio_Arrendado( {const } )
    @mediation
    [1] -- [1]  Contrato_de_Arrendamento_de_Bens( {const } )
}
kind Malha_Ferroviria {
    @componentOf
    [1] <o>--  [2..*]  Via_Frrea
    @componentOf
    [1] <o>--  [2..*]  Trecho_Ferrovirio
}
role Agncia_Reguladora_Concedente  specializes Agncia_Reguladora{
    @mediation
    ( {const } )[1] --  [1..*]  Outorga_de_Concesso_Ferroviria_Ativa
}
role Malha_Ferroviria_Concedida  specializes Malha_Ferroviria
relator Outorga_de_Concesso_Ferroviria_com_Contrato_de_Concesso  specializes Outorga_de_Concesso_Ferroviria_Ativa
role Contrato_de_Concesso_Ferroviria_nico  specializes Contrato_de_Concesso_Ferroviria{
    @mediation
    ( {const } contrato )[1] -- [1]  Outorga_de_Concesso_Ferroviria_com_Contrato_de_Concesso
}
subkind Contrato_de_Concesso_Ferroviria  specializes Contrato_de_Outorga{
    @componentOf
    [1] <o>-- [1]  Plano_de_Investimento
}
role Contrato_de_Concesso_Ferroviria_com_Contrato_de_Arrendamento_de_Bens  specializes Contrato_de_Concesso_Ferroviria{
    @mediation
    ( {const } )[1] -- [1]  Outorga_de_Concesso_Ferroviria_com_Contrato_de_Concesso_e_Contrato_de_Arrendamento_de_Bens
}
subkind Contrato_de_Arrendamento_de_Bens  specializes Contrato

