import Georreferenciamento
import Poder_Legislativo
import Projeto_de_Ato_Normativo

package Senado_Federal

relator Mandato_de_Senador {
    @mediation
     [1..*] -- [1]  Unidade_Federativa( {const } )
    @mediation
    [1] -- [1]  Senador( {const } )
}
role Senador  specializes Parlamentar{
    @material
     [1..*] -- representa -- [1]  Unidade_Federativa
}
collective Senado_Federal  specializes Agente_Legislativo{
    senadores : undefined
    @memberOf
    [1] <o>--  [2..*]  Senador
}

