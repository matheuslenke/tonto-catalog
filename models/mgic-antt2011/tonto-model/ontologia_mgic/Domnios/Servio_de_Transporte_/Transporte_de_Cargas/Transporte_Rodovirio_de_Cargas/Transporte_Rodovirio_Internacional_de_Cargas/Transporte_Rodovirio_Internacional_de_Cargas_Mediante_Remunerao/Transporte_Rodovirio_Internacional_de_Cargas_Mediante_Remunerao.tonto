relator Operao_de_Transporte_Rodovirio_Internacional_de_Cargas_Mediante_Remunerao  specializes Operao_de_Transporte_Rodovirio_Internacional_de_Cargas{
    @mediation
     [1..*] -- [1]  Contratante_de_Prestao_de_Servio_de_Transporte_Rodovirio_Internacional_de_Cargas( {const } )
