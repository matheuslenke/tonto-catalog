import Licena_Originria
import Leilo_de_Licitao

package Licena_Complementar

role Empresa_Estrangeira_Indicadora_de_Representante  specializes Empresa_Indicadora_de_Representante
relator Indicao_de_Representante_para_Pedido_de_Licena_Complementar  specializes Indicao_de_Representante{
    @mediation
    [1] --  [0..1]  Procurao_para_Indicao_de_Representante_para_Pedido_de_Licena_Complementar( {const } procuraoIndicaoRepresentanteLicenaComplementa )
    @mediation
    [1] -- [1]  Empresa_Estrangeira_Indicadora_de_Representante( {const } empresaEstrangeira )
    @mediation
    [1] -- [1]  Representante_Legal_de_Empresa_Estrangeira( {const } representanteLegalBrasileiro )
}
roleMixin Representante_de_Empresa_Estrangeira  specializes Redespachante
relator Licena_Complementar {
    validade : undefined
    @mediation
    [1] -- [1]  Redespachante( {const } )
    @mediation
     [0..1] -- [1]  Licena_Originria( {const } )
    @mediation
    [1] -- [1]  Procurao_para_Indicao_de_Representante_para_Pedido_de_Licena_Complementar( {const } )
}
role Pessoa_Jurdica_Representante_de_Empresa_Estrangeira  specializes Representante_de_Empresa_Estrangeira
roleMixin Redespachante 
role Pessoa_Fsica_Representante_de_Empresa_Estrangeira  specializes Representante_de_Empresa_Estrangeira
role Representante_Legal_de_Empresa_Estrangeira  specializes Redespachante, Representante_Legal
role Procurao_para_Indicao_de_Representante_para_Pedido_de_Licena_Complementar  specializes Procurao_para_Indicao_de_Representante

