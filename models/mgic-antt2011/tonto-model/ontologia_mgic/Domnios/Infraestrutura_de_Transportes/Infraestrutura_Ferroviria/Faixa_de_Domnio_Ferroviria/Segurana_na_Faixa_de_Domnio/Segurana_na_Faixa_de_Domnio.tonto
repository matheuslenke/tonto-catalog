import Pessoa_Jurdica
import Faixa_de_Domnio_Ferroviria
import Pessoa
import Concesso_Ferroviria

package Segurana_na_Faixa_de_Domnio

role Sociedade_Empresria_Contratante_de_Servio_de_Vigilncia  specializes Sociedade_Empresria
role Faixa_de_Domnio_Ferroviria_com_Vigilncia  specializes Faixa_de_Domnio_Ferroviria
roleMixin Invasor  specializes Pessoa
relator Invaso_de_Faixa_de_Domnio_Ferroviria {
    @mediation
    [1] --  [1..*]  Invasor( {const } )
}
role Concessionria_Ferroviria_que_Efetua_Servio_de_Vigilncia  specializes Concessionria_Ferroviria
relator Servio_de_Vigilncia_Ferroviria_Contratado_por_Terceiros  specializes Servio_de_Vigilncia_Ferroviria{
    @mediation
     [2..*] -- [1]  Sociedade_Empresria_Contratante_de_Servio_de_Vigilncia( {const } )
}
relator Servio_de_Vigilncia_Ferroviria {
    @mediation
    [1] -- [1]  Faixa_de_Domnio_Ferroviria_com_Vigilncia( {const } )
}
relator Servio_de_Vigilncia_Ferroviria_Efetuado_pela_Concessionria  specializes Servio_de_Vigilncia_Ferroviria{
    @mediation
    [1] -- [1]  Concessionria_Ferroviria_que_Efetua_Servio_de_Vigilncia( {const } )
}
role Faixa_de_Domnio_Ferroviria_com_Invaso  specializes Faixa_de_Domnio_Ferroviria{
    @mediation
    [1] -- [1]  Invaso_de_Faixa_de_Domnio_Ferroviria( {const } )
}

