}
relator Fiscalizao_de_PEF_de_Contrato  specializes Fiscalizao_de_PEF{
    @mediation
     [1..*] --  [1..*]  Contrato_de_Prestao_de_ST_Fiscalizado_por_PEF( {const } )
}
