import Participao_da_Sociedade
import Reajuste_Tarifrio
import Concesso_Ferroviria

package Reviso_Tarifria

role Concessionria_Adimplente_que_Solicita_Reviso  specializes Concessionria_Adimplente
role Tabela_Tarifria_Criada_a_Partir_de_Reviso_Iniciada_por_Concedente  specializes Tabela_Tarifria_Criada_a_Partir_de_Reviso
relator Reviso_Solicitada_por_Concessionria_Ferroviria  specializes Reviso_de_Tabela_Tarifria{
    @mediation
     [1..*] -- [1]  Concessionria_Adimplente_que_Solicita_Reviso( {const } )
}
relator Reviso_de_Tabela_Tarifria {
    data_da_reviso : undefined
    @mediation
    [1] -- [1]  Agncia_Reguladora_Avaliadora_de__Reviso_( {const } )
}
role Agncia_Reguladora_Avaliadora_de__Reviso_  specializes Agncia_Reguladora_Concedente
role Tabela_Tarifria_Criada_a_Partir_de_Reviso_Iniciada_por_Concessionria_Ferroviria  specializes Tabela_Tarifria_Criada_a_Partir_de_Reviso
relator Reviso_Tarifria_Aceita  specializes Reviso_Solicitada_por_Concessionria_Ferroviria{
    @mediation
    [1] -- [1]  Tabela_Tarifria_Criada_a_Partir_de_Reviso_Iniciada_por_Concessionria_Ferroviria( {const } )
}
mode Justificativa_de_Reviso_Extraordinria {
    @characterization
    [1] -- [1]  Reviso_Extraordinria_de_Tabela_Tarifria( {const } )
}
role Concedente_Solicitante_de_Reviso  specializes Agncia_Reguladora_Concedente{
    @mediation
    ( {const } )[1] --  [1..*]  Reviso_Iniciada_por_Concedente_
}
relator Reviso_Iniciada_por_Concedente_  specializes Reviso_de_Tabela_Tarifria{
    @mediation
    [1] -- [1]  Tabela_Tarifria_Criada_a_Partir_de_Reviso_Iniciada_por_Concedente( {const } )
}
relator Reviso_Ordinria_de_Tabela_Tarifria  specializes Reviso_de_Tabela_Tarifria{
    vigncia : undefined
    @mediation
    [1] -- [1]  Audincia_Pblica( {const } )
}
relator Reviso_Tarifria_Negada  specializes Reviso_Solicitada_por_Concessionria_Ferroviria
relator Reviso_Extraordinria_de_Tabela_Tarifria  specializes Reviso_de_Tabela_Tarifria

