import Tipo_de_Veculo

package Combinao_de_Veculos

kind Combinao_de_Veculo {
    pbtm : undefined
    @componentOf
    [1] <o>--  [1..*]  Unidade_Rebocada
    @componentOf
    [1] <o>-- [1]  Unidade_Tratora
}
subkind Combinao_de_Veculo_de_Passageiro  specializes Combinao_de_Veculo
role Unidade_Tratora  specializes Veculo_Rodovirio_Automotor
subkind Combinao_de_Veculo_de_Carga  specializes Combinao_de_Veculo
role Unidade_Rebocada  specializes Veculo_Rodovirio_Rebocado

