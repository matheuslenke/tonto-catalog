import Georreferenciamento
import Projeto_de_Ato_Normativo
import Poder_Legislativo

package Cmara_dos_Deputados

relator Mandato_de_Deputado_Federal {
    @mediation
    [1] -- [1]  Deputado_Federal( {const } )
}
collective Cmara_dos_Deputados  specializes Agente_Legislativo{
    @memberOf
    [1] <o>--  [2..*]  Deputado_Federal
}
role Deputado_Federal  specializes Parlamentar{
    @material
     [1..*] -- representa -- [1]  Unidade_Federativa
}

