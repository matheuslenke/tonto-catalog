    @mediation
     [1..*] -- [1]  Agncia_Reguladora_Arbitradora( {const } )
}
role Agncia_Reguladora_Arbitradora 
relator Viagem_com_Interseo_com_Conflito_Mediado  specializes Viagem_Ferroviria_com_Interseo{
    @mediation
