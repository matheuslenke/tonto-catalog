import Pessoa
import Imvel

package Estabelecimento

relator Registro_de_Estabelecimento {
    @mediation
    [1] --  [1..*]  Estabelecimento( {const } )
    @mediation
     [1..*] -- [1]  Pessoa_Jurdica( {const } )
}
role Matriz  specializes Pessoa_Jurdica{
    denominao : undefined
    @componentOf
    [1] <o>--  [0..*]  Filial
}
roleMixin Estabelecimento  specializes Imvel
role Filial  specializes Pessoa_Jurdica

