package AndreaAlbuquerque_Biodiversity

kind Person  specializes Animal
role ObjectClassifier  specializes Person{
    @mediation
    ( {const } )[1] --  [1..*]  Classification( {const } )
}
role ParticipantInCollection  specializes Person{
    @mediation
    ( {const } ) [1..*] -- [1]  Collection( {const } )
}
role Researcher  specializes Person{
    @mediation
    ( {const } )[1] --  [1..*]  InstitutionalContract( {const } )
    @material
     [1..*] -- worksin --  [1..*]  ResearchInstitution( {const } )
}
role AssistantInCollection  specializes ParticipantInCollection
role ResearcherParticipant  specializes ParticipantInCollection, Researcher
role ResponsibleForCollection  specializes ResearcherParticipant
category SpatialLocation {
    [*] --  [1..*]  GeographicCoordinate
}
role CollectionLocation  specializes SpatialLocation{
    @mediation
    [1] --  [1..*]  Collection( {const } )
}
relator Classification {
    @mediation
     [1..*] -- [1]  Species( {const } )
    @mediation
    [1] -- [1]  ClassifiedEntity( {const } )
}
type Species  specializes Taxon{
    @comparative
     [1..*] -- speciesIdentity --  [1..*]  Species
}
role ClassifiedEntity  specializes EntityInStudy
role CollectedEntity  specializes EntityInStudy
role EntityInStudy  specializes BioticEntity
category BioticEntity  specializes MaterialEntity{
    size : undefined
    weight : undefined
    @componentOf
     [1..*]  --<o>  [1..*]  Ecosystem
    [*] -- instanceOf --  [1..*]  Taxon
    @comparative
    [1] -- [1]  LifeStage
}
relator Collection {
    EndDate : undefined
    StartDate : undefined
    @mediation
    [1] -- [1]  ResponsibleForCollection( {const } )
    @mediation
    [1] --  [1..*]  CollectedEntity( {const } )
}
relator InstitutionalContract {
    @mediation
     [1..*] -- [1]  ResearchInstitution( {const } )
}
kind ResearchInstitution {
    acronym : undefined
    name : undefined
}
role ResponsibleInstitution  specializes ResearchInstitution{
    @mediation
    ( {const } )[1] -- [1]  Collection( {const } )
}
relator CollectionThroughFraction  specializes Collection
relator RandomCollection  specializes Collection
relator ManualCollection  specializes Collection
relator InstrumentedCollection  specializes Collection
role CollectionInstrument  specializes Instrument{
    @mediation
    ( {const } ) [1..*] -- [1]  InstrumentedCollection( {const } )
}
kind Instrument 
