@prefix : <https://example.com#>.
@prefix gufo: <http://purl.org/nemo/gufo#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

<https://example.com> rdf:type owl:Ontology;
    owl:imports gufo:.
:BiologicalPerson rdf:type owl:NamedIndividual;
    rdfs:label "BiologicalPerson"@en.
:ElectricPotential rdf:type owl:Class, gufo:Kind, owl:NamedIndividual;
    rdfs:subClassOf gufo:Quantity;
    rdfs:label "ElectricPotential"@en.
:PersonUnderECGMonitoring rdf:type owl:Class, gufo:Role, owl:NamedIndividual;
    rdfs:label "PersonUnderECGMonitoring"@en.
:ECGRecordingSession rdf:type owl:NamedIndividual;
    rdfs:label "ECGRecordingSession"@en.
:ECGDevice rdf:type owl:Class, gufo:SubKind, owl:NamedIndividual;
    rdfs:subClassOf gufo:FunctionalComplex;
    rdfs:label "ECGDevice"@en.
:ECGLead rdf:type owl:Class, gufo:Kind, owl:NamedIndividual;
    rdfs:subClassOf gufo:FunctionalComplex;
    rdfs:label "ECGLead"@en.
:ECGLeadUnipolar rdf:type owl:Class, gufo:SubKind, owl:NamedIndividual;
    rdfs:label "ECGLeadUnipolar"@en.
:ECGLeadBipolarLimb rdf:type owl:Class, gufo:SubKind, owl:NamedIndividual;
    rdfs:label "ECGLeadBipolarLimb"@en.
:TimeSeriesMeasurements rdf:type owl:Class, gufo:Kind, owl:NamedIndividual;
