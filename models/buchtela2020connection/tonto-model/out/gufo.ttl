    rdfs:subClassOf gufo:FunctionalComplex;
    rdfs:label "Exerciseci"@en.
:ki_passed rdf:type owl:Class, gufo:Phase, owl:NamedIndividual;
    rdfs:label "ki_passed"@en.
:ki_non_passed rdf:type owl:Class, gufo:Phase, owl:NamedIndividual;
    rdfs:label "ki_non_passed"@en.
:p_passed rdf:type owl:Class, gufo:Phase, owl:NamedIndividual;
    rdfs:label "p_passed"@en.
:p_non_passed rdf:type owl:Class, gufo:Phase, owl:NamedIndividual;
    rdfs:label "p_non_passed"@en.
:t_passed rdf:type owl:Class, gufo:Phase, owl:NamedIndividual;
    rdfs:label "t_passed"@en.
:t_non_passed rdf:type owl:Class, gufo:Phase, owl:NamedIndividual;
    rdfs:label "t_non_passed"@en.
:ci_passed rdf:type owl:Class, gufo:Phase, owl:NamedIndividual;
    rdfs:label "ci_passed"@en.
:ci_non_passed rdf:type owl:Class, gufo:Phase, owl:NamedIndividual;
    rdfs:label "ci_non_passed"@en.
:Student rdfs:subClassOf :Person.
:Teacher rdfs:subClassOf :Person.
:Credit_non_passed rdfs:subClassOf :Course.
:Course_started rdfs:subClassOf :Course.
:Credit_passed rdfs:subClassOf :Course.
:ki_passed rdfs:subClassOf :Chapterki.
:ki_non_passed rdfs:subClassOf :Chapterki.
:p_non_passed rdfs:subClassOf :Projectp.
:p_passed rdfs:subClassOf :Projectp.
:t_passed rdfs:subClassOf :Testt.
:t_non_passed rdfs:subClassOf :Testt.
