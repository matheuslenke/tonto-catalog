     [1..*] -- schemamanufacturer --  [1..*]  organisation
}
subkind ManufacturingSchedule  specializes Schedule
subkind ProcurementSchedule  specializes Schedule
subkind WorkPreparationSchedule  specializes Schedule
subkind AssemblySchedule  specializes ConstructionSchedule
subkind ConstructionSchedule  specializes Schedule
subkind EngineeringSchedule  specializes Schedule
category Agent  specializes Agent, Agent{
    [*] -- hasAgent -- [*]  activity
}
disjoint genset Disjoined{
    general Entity
    specifics DynamicEntity, SingularEntity 
}
disjoint genset Disjoined{
    general Component
    specifics element, Assembly 
}
disjoint genset Disjoined{
    general StructuredValue
    specifics List2D, Interval, Entry2D 
}
disjoint genset Disjoined{
    general ClassifiedObject
    specifics Attribute, RangedAttribute, componentConnection, Component 
}
disjoint genset Disjoined{
    general PropertyState
    specifics deleted, CurrentPropertyState, OutdatedPropertyState 
}
disjoint genset disjoined{
    general Event
    specifics ConceptDevelopment, ProjectDevelopment 
}

