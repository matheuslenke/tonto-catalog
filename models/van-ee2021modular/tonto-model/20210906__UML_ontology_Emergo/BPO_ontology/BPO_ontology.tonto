import opm
import omg
import bpo
import schema
import dice
import seas
import bot
import ifc

package BPO_ontology

quality Property  specializes Property, Property, Property{
    @material
     [0..*] -- omgisExplicitlyDerivedFrom --  [0..*]  Property
    @material
     [0..*] -- omghasInferredPropertyState --  [0..*]  PropertyState
    @material
     [0..*] -- opmhasPropertyState --  [0..*]  PropertyState
    @material
     [0..*] -- omgisImplicitlyDerivedFrom --  [0..*]  Geometry
}
event ProjectDevelopment  specializes Event
subkind Housing_Block  specializes Building
subkind House  specializes Building{
    specialCareFacilityHome : boolean
    cornerhouse : boolean
    Mirrored : boolean
    adresHouseNumber : undefined
    streetName : string
    zipCode : double
    city : string
    country : string
    @componentOf
     [3..*]  --<o> [1]  Housing_Block
}
kind Module  specializes Assembly{
    @componentOf
     [1..*]  --<o> [1]  House
}
relator describesAttribute {
    @mediation
     [0..*] -- [1]  List2D( {const } )
    @mediation
     [0..*] -- [1]  RangedAttribute( {const } )
}
subkind describedAttributeInFirstColumn  specializes describesAttribute
subkind describedAttributeInSecondColumn  specializes describesAttribute
event WIP_project  specializes ProjectDevelopment
event Finished_project  specializes ProjectDevelopment
subkind E_Wanden  specializes BuildingElement
subkind F_Vloeren  specializes BuildingElement
