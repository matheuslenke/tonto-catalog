import Aviation_Safety

package UFO

class Trope {
    [*] -- inheres_in -- [*]  Physical_Object
}
class Object {
    [*] -- has_object_part -- [*]  Object
    [*] -- is_participant_of -- [*]  Event
}
class Event {
    [*] -- has_participant -- [*]  Agent
}
class Action  specializes Event{
    [*] -- is_performed_by -- [*]  Agent
    [*] -- is_performed_by -- [*]  Cabin_Crew
}
class Agent  specializes Object

